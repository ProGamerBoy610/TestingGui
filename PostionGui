Library:CreateSection(SettingsTab, "Window Settings")
Library:CreateSpace(SettingsTab, 4)
Library:CreateToggle(SettingsTab, {
    Title = "Window Transparency",
    Desc = "Toggle window transparency on/off",
    Default = false,
    Callback = function(state)
        Library:ToggleTransparency(state)
        print("Transparency:", state)
    end
})
Library:CreateToggle(SettingsTab, {
    Title = "Sound Effects",
    Desc = "Enable UI sound effects",
    Default = true,
    Callback = function(state) print("Sound Effects:", state) end
})
Library:CreateDivider(SettingsTab)
Library:CreateSection(SettingsTab, "Keybind Settings")
Library:CreateSpace(SettingsTab, 4)
Library:CreateInput(SettingsTab, {
    Title = "Toggle UI Keybind",
    Desc = "Type ONE letter or key (e.g., Q or RightControl)",
    Value = "RightControl",
    Placeholder = "Enter key...",
    Callback = function(input)
        input = input:gsub("^%s*(.-)%s*$", "%1")
        
        local keyCode
        
        if #input == 1 then
            local upperInput = input:upper()
            keyCode = Enum.KeyCode[upperInput]
        else
            local success, result = pcall(function()
                return Enum.KeyCode[input]
            end)
            if success then
                keyCode = result
            end
        end
        
        if keyCode then
            Library:SetToggleKey(keyCode)
            print("Keybind set to:", input)
        else
            print("Invalid key:", input)
        end
    end
})
Library:CreateDivider(SettingsTab)
Library:CreateSection(SettingsTab, "Interface Settings")
Library:CreateSpace(SettingsTab, 4)
Library:CreateToggle(SettingsTab, {
    Title = "Auto Save Settings",
    Desc = "Automatically save configuration",
    Default = false,
    Callback = function(state) print("Auto Save:", state) end
})
Library:CreateSlider(SettingsTab, {
    Title = "UI Scale",
    Step = 5,
    Value = { Min = 80, Max = 120, Default = 100 },
    Callback = function(value) print("UI Scale:", value .. "%") end
})
Library:CreateDivider(SettingsTab)
Library:CreateButton(SettingsTab, {
    Title = "Reset All Settings",
    Desc = "Restore default configuration",
    Callback = function() print("Settings reset to defaults!") end
})
Library:CreateSpace(SettingsTab, 4)
Library:CreateLabel(SettingsTab, "VERSION 2.0.0")

