    end
    local function vu201()
        local v188 = vu40()
        if # v188 == 0 then
            return false
        end
        local v189, v190, v191 = ipairs(v188)
        local v192 = {}
        while true do
            local v193
            v191, v193 = v189(v190, v191)
            if v191 == nil then
                local vu194
                if # v192 <= 0 then
                    vu194 = v188[math.random(1, # v188)]
                else
                    vu194 = v192[math.random(1, # v192)]
                end
                local v195 = {
                    vu194
                }
                _G.SelectedAreas = v195
                _G.FilterByArea = true
                pcall(function()
                    if vu2 then
                        vu2:Notify({
                            Title = "Switched Area",
                            Content = "Now mining: " .. tostring(vu194),
                            Duration = 2
                        })
                    end
                end)
                return true
            end
            local v196, v197, v198 = ipairs(_G.SelectedAreas)
            local v199 = false
            while true do
                local v200
                v198, v200 = v196(v197, v198)
                if v198 == nil then
                    break
                end
                if vu49(v200) == vu49(v193) then
                    v199 = true
                    break
                end
            end
            if not v199 then
                table.insert(v192, v193)
            end
        end
    end
