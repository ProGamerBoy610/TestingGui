        end
    end
    local function vu60(pu52)
        if pu52 then
            local v53, v54 = pcall(function()
                return pu52:GetAttribute("Health")
            end)
            if v53 and type(v54) == "number" then
                return v54
            else
                local vu55 = vu51(pu52)
                if vu55 then
                    local v56, v57 = pcall(function()
                        return vu55:GetAttribute("Health")
                    end)
                    if v56 and type(v57) == "number" then
                        return v57
                    end
                    local v58 = vu55:FindFirstChild("Health")
                    if v58 and v58.Value ~= nil then
                        return tonumber(v58.Value)
                    end
                end
                local v59 = pu52:FindFirstChild("Health")
                if v59 and v59.Value ~= nil then
                    return tonumber(v59.Value)
                else
                    return nil
                end
            end
        else
            return nil
        end
    end
    local function vu71(pu61)
        if not pu61 then
            return nil
        end
        local v62, v63 = pcall(function()
            return pu61:GetAttribute("RockType")
        end)
        if v62 and (v63 and vu22[tostring(v63)]) then
            return tostring(v63)
