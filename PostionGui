                    if v510 then
                        local v511, v512, v513 = ipairs(v510:GetDescendants())
                        while true do
                            local v514
                            v513, v514 = v511(v512, v513)
                            if v513 == nil then
                                break
                            end
                            if v514:IsA("BasePart") then
                                v514.CanCollide = false
                            end
                        end
                    end
                end)
                vu475 = true
            end
        end
    end
    local function vu521()
