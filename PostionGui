    })
    v600:Divider()
    _G.AutoDrinkPotionsEnabled = false
    _G.AutoDrinkSelectedPotions = {
        "HealthPotion1"
    }
    _G.AutoDrinkDelay = 0.8
    _G.AutoDrinkToolRF = nil
    _G.AutoDrinkCancel = false
    _G._AutoDrinkCoroutine = nil
    _G.AutoDrinkHealthEnabled = false
    _G.AutoDrinkHealthThreshold = 45
    function _G.FindToolServiceRF()
        if _G.AutoDrinkToolRF then
            return _G.AutoDrinkToolRF
        end
        local v679, v680 = pcall(function()
            local v675 = rep:FindFirstChild("Shared") and rep.Shared:FindFirstChild("Packages")
            if v675 then
                v675 = rep.Shared.Packages:FindFirstChild("Knit")
            end
            if v675 and v675:FindFirstChild("Services") and v675.Services:FindFirstChild("ToolService") then
                local v676 = v675.Services.ToolService:FindFirstChild("RF")
                if v676 and v676:FindFirstChild("ToolActivated") then
                    return v676.ToolActivated
                end
            end
            local v677, v678 = pcall(function()
                return rep.Shared.Packages.Knit.Services.ToolService.RF.ToolActivated
            end)
            return v677 and v678 and v678 or nil
        end)
        if not (v679 and v680) then
            return nil
        end
        _G.AutoDrinkToolRF = v680
        return v680
    end
    function _G.TryDrinkPotion(pu681)
        if not pu681 then
            return false, "no-name"
        end
        local vu682 = _G.FindToolServiceRF()
        if not vu682 then
            return false, "no-ToolActivated-remote"
        end
        local v683, v684 = pcall(function()
            return vu682:InvokeServer(pu681)
        end)
        return v683, v684
    end
    local function vu688(p685, p686)
        local v687 = p686 or 0
        if type(p685) == "number" then
            return p685
        end
        if type(p685) == "table" then
            if p685.Value and type(p685.Value) == "number" then
                return p685.Value
            end
            if p685.Default and type(p685.Default) == "number" then
                return p685.Default
            end
        end
        return v687
    end
    function _G.StartAutoDrinkLoop()
        if not _G._AutoDrinkCoroutine then
            _G.AutoDrinkPotionsEnabled = true
            _G.AutoDrinkCancel = false
            _G._AutoDrinkCoroutine = task.spawn(function()
                while _G.AutoDrinkPotionsEnabled and not _G.AutoDrinkCancel do
                    if game:GetService("Players").LocalPlayer and game:GetService("Players").LocalPlayer.Character then
                        game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
                    end
                    if type(_G.AutoDrinkSelectedPotions) ~= "table" or # _G.AutoDrinkSelectedPotions <= 0 then
                        task.wait(0.25)
                    else
                        local v689, v690, v691 = ipairs(_G.AutoDrinkSelectedPotions)
                        while true do
                            local v692
                            v691, v692 = v689(v690, v691)
                            if v691 == nil or (_G.AutoDrinkCancel or not _G.AutoDrinkPotionsEnabled) then
                                break
                            end
                            local v693 = tostring(v692)
                            local _, _ = _G.TryDrinkPotion(v693)
                            task.wait(math.max(0.05, tonumber(_G.AutoDrinkDelay) or 0.8))
                        end
                    end
                end
                _G._AutoDrinkCoroutine = nil
                _G.AutoDrinkCancel = false
                _G.AutoDrinkPotionsEnabled = false
            end)
        end
    end
    function _G.StopAutoDrinkLoop()
        _G.AutoDrinkCancel = true
        _G.AutoDrinkPotionsEnabled = false
        _G._AutoDrinkCoroutine = nil
    end
    _G._AutoDrinkHealthCoroutine = nil
    function _G.StartAutoDrinkHealthLoop()
        if not _G._AutoDrinkHealthCoroutine then
            _G._AutoDrinkHealthCoroutine = task.spawn(function()
                while _G.AutoDrinkHealthEnabled do
                    local v694 = game:GetService("Players").LocalPlayer
                    if v694 then
                        v694 = v694.Character
                    end
                    if v694 then
                        v694 = v694:FindFirstChildOfClass("Humanoid")
                    end
                    if v694 and v694.Health > 0 and v694.Health / v694.MaxHealth * 100 <= (_G.AutoDrinkHealthThreshold or 45) then
                        _G.TryDrinkPotion("HealthPotion1")
                        task.wait(math.max(0.15, _G.AutoDrinkDelay or 0.8))
                    end
                    task.wait(0.15)
                end
                _G._AutoDrinkHealthCoroutine = nil
            end)
        end
    end
    function _G.StopAutoDrinkHealthLoop()
        _G.AutoDrinkHealthEnabled = false
    end
    if v600 then
        v600:Dropdown({
            Title = "Auto-Drink Potions (multi)",
            Desc = "Potions that the auto-drink loop will attempt to use (order matters).",
            Values = (function()
                local v695, v696, v697 = ipairs(_G.POTION_OPTIONS or {})
