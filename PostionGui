local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()

local window = DrRayLibrary:Load("Fryzer Hub", "Default")

local tab = DrRayLibrary.newTab("Main", "ImageIdHere")

tab.newButton("Button", "", function()
    print('Hello!')
end)

tab.newToggle("Toggle", "Toggle! (prints the state)", true, function(toggleState)
    if toggleState then
        print("On")
    else
        print("Off")
    end
end)

local tab = DrRayLibrary.newTab("Beta", "ImageIdHere")


tab.newSlider("Slider", "Epic slider", 1000, false, function(num)
    print(num)
end)

window:Toggle()

window:Open()

window:Close()

