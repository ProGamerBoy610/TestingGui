HeaderCorner.Parent = HeaderFrame

local GradientBar = Instance.new("Frame")
GradientBar.Name = "GradientBar"
GradientBar.Parent = HeaderFrame
GradientBar.BackgroundColor3 = Config.Colors.Purple
GradientBar.BorderSizePixel = 0
GradientBar.Size = UDim2.new(1, 0, 0, 2)
GradientBar.Position = UDim2.new(0, 0, 1, -2)

local GradientBarGradient = Instance.new("UIGradient")
GradientBarGradient.Parent = GradientBar
GradientBarGradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Config.Colors.Purple),
    ColorSequenceKeypoint.new(0.25, Config.Colors.Red),
    ColorSequenceKeypoint.new(0.5, Config.Colors.Orange),
    ColorSequenceKeypoint.new(0.75, Config.Colors.Yellow),
    ColorSequenceKeypoint.new(1, Config.Colors.White)
}

spawn(function()
    while ScreenGui.Parent do
        for i = 0, 1, 0.01 do
            if not ScreenGui.Parent then break end
            GradientBarGradient.Offset = Vector2.new(i, 0)
            wait(0.03)
        end
        for i = 1, 0, -0.01 do
            if not ScreenGui.Parent then break end
            GradientBarGradient.Offset = Vector2.new(i, 0)
            wait(0.03)
        end
    end
end)

