-- ==================== TAB 7: MIXED COMPONENTS ====================
local Tab7 = Window:Tab({
    Title = "Mixed",
    Icon = "grid-3x3",
    Locked = false,
})

local Section13 = Tab7:Section({
    Title = "All Components Together",
    Icon = "layers",
    Opened = true,
})

Tab7:Button({
    Title = "Mixed Button",
    Desc = "Button in mixed tab",
    Locked = false,
    Callback = function()
        print("Mixed button")
    end
})

Tab7:Toggle({
    Title = "Mixed Toggle",
    Desc = "Toggle in mixed tab",
    Icon = "zap",
    Type = "Switch",
    Default = false,
    Callback = function(state)
        print("Mixed toggle: " .. tostring(state))
    end
})

Tab7:Slider({
    Title = "Mixed Slider",
    Step = 1,
    Value = {
        Min = 0,
        Max = 100,
        Default = 50,
    },
    Callback = function(value)
        print("Mixed slider: " .. value)
    end
})

Tab7:Dropdown({
    Title = "Mixed Dropdown",
    Values = { "A", "B", "C" },
    Value = "A",
    Callback = function(option)
        print("Mixed dropdown: " .. option)
    end
})

Tab7:Input({
    Title = "Mixed Input",
    Desc = "Input in mixed tab",
    Value = "",
    InputIcon = "edit",
    Type = "Input",
    Placeholder = "Type here...",
    Callback = function(input)
        print("Mixed input: " .. input)
    end
})

Tab7:Divider()

Tab7:Paragraph({
    Title = "Mixed Paragraph",
    Desc = "Paragraph in mixed tab",
    Color = "Purple",
    Locked = false,
    Buttons = {
        {
            Icon = "info",
            Title = "Info",
            Callback = function()
                print("Info")
            end,
        }
    }
})

Tab7:Space()

-- ==================== WINDOW LEVEL COMPONENTS ====================
Window:Divider()

local WindowSection = Window:Section({
    Title = "Window Level Section",
    Icon = "window",
    Opened = true,
})

Window:Space()

-- Final Notification
WindUI:Notify({
    Title = "All Components Loaded",
    Content = "Every component is now visible!",
    Duration = 5,
    Icon = "check-circle",
})
