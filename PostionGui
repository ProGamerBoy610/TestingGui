    local function vu405()
        if vu376 then
            return vu376
        end
        local v403, v404 = pcall(function()
            local v402 = RS and (RS.Shared and RS.Shared.Packages) and (RS.Shared.Packages.Knit and RS.Shared.Packages.Knit.Services and (RS.Shared.Packages.Knit.Services.DialogueService and RS.Shared.Packages.Knit.Services.DialogueService.RE))
            if v402 then
                v402 = RS.Shared.Packages.Knit.Services.DialogueService.RE.DialogueEvent
            end
            return v402
        end)
        if not (v403 and v404) then
            return nil
        end
        vu376 = v404
        return v404
    end
    local function vu410()
        local vu406 = vu397()
        if vu406 then
            local vu407 = workspace.Proximity
            if vu407 then
                vu407 = workspace.Proximity["Greedy Cey"]
            end
            if vu407 then
                for _ = 1, vu387 do
                    if not _G.AutoSellEnabled then
                        return false, "stopped"
                    end
                    local v408, v409 = pcall(function()
                        return vu406:InvokeServer(vu407)
                    end)
                    if v408 and v409 ~= false then
                        return true, v409
                    end
                    task.wait(vu388)
                end
                return false, "timeout"
            else
                warn("[AutoSell] workspace.Proximity[\'Greedy Cey\'] not found.")
                return false, "no-part"
            end
        else
            warn("[AutoSell] Proximity Dialogue remote not found.")
            return false, "no-prox-remote"
        end
    end
    local function vu417(p411, p412)
        local vu413 = vu401()
        if not vu413 then
            warn("[AutoSell] RunCommand remote not found; cannot sell:", p411)
            return false, "no-run-remote"
        end
        local vu414 = {
            [p411] = p412 or 1
        }
        for _ = 1, vu389 do
            if not _G.AutoSellEnabled then
                return false, "stopped"
            end
            local v415, v416 = pcall(function()
                return vu413:InvokeServer("SellConfirm", {
                    Basket = vu414
                })
            end)
            if v415 then
                return true, v416
            end
            task.wait(vu390)
        end
        return false, "sell-failed"
    end
    local function vu422()
