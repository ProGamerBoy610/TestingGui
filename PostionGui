    _G.Section = _G.RaceTab:Section({
        Title = "Reroll race",
        Icon = "refresh-ccw"
    })
    if not _G.__AUTO_REROLL_LOADED then
        _G.__AUTO_REROLL_LOADED = true
        _G.Players = game:GetService("Players")
        _G.ReplicatedStorage = game:GetService("ReplicatedStorage")
