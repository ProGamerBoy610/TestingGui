        end
    end
    local function vu49(p48)
        return not p48 and "" or string.lower(tostring(p48))
    end
    local function vu51(p50)
        if p50 then
            if p50:IsA("BasePart") then
                return p50
            elseif p50.PrimaryPart then
                return p50.PrimaryPart
            else
                return p50:FindFirstChildWhichIsA("BasePart")
            end
        else
            return nil
        end
    end
    local function vu60(pu52)
        if pu52 then
            local v53, v54 = pcall(function()
                return pu52:GetAttribute("Health")
            end)
            if v53 and type(v54) == "number" then
                return v54
            else
                local vu55 = vu51(pu52)
                if vu55 then
                    local v56, v57 = pcall(function()
                        return vu55:GetAttribute("Health")
                    end)
                    if v56 and type(v57) == "number" then
                        return v57
                    end
                    local v58 = vu55:FindFirstChild("Health")
                    if v58 and v58.Value ~= nil then
                        return tonumber(v58.Value)
                    end
                end
                local v59 = pu52:FindFirstChild("Health")
                if v59 and v59.Value ~= nil then
                    return tonumber(v59.Value)
                else
                    return nil
                end
            end
        else
            return nil
        end
    end
    local function vu71(pu61)
        if not pu61 then
            return nil
        end
        local v62, v63 = pcall(function()
            return pu61:GetAttribute("RockType")
        end)
        if v62 and (v63 and vu22[tostring(v63)]) then
            return tostring(v63)
        end
        local v64, v65 = pcall(function()
            return pu61:GetAttribute("Type")
        end)
        if v64 and (v65 and vu22[tostring(v65)]) then
            return tostring(v65)
        end
        if vu22[pu61.Name] then
            return pu61.Name
        end
        local v66 = vu49(pu61.Name or "")
        local v67, v68, v69 = pairs(vu22)
        while true do
            local v70
            v69, v70 = v67(v68, v69)
            if v69 == nil then
                break
            end
            if string.find(v66, vu49(v69), 1, true) then
                return v69
            end
        end
        return nil
    end
    local function vu83(p72)
        if not p72 then
            return false
        end
        if not _G.MineTheseOres or # _G.MineTheseOres == 0 then
            return true
        end
        local v73 = vu71(p72)
        if not v73 then
            return false
        end
        local v74 = vu22[v73]
        if not v74 then
            return false
        end
        local v75, v76, v77 = ipairs(_G.MineTheseOres)
        while true do
            local v78
            v77, v78 = v75(v76, v77)
            if v77 == nil then
                break
            end
            local v79, v80, v81 = ipairs(v74)
            while true do
                local v82
                v81, v82 = v79(v80, v81)
                if v81 == nil then
                    break
                end
                if vu49(v82) == vu49(v78) then
                    return true
                end
            end
        end
        return false
    end
    local function vu87(p84)
        if not p84 then
            return false
        end
        local v85 = _G.MineTheseOres and _G.MineTheseOres[1] or nil
        if not v85 then
            return false
        end
        if vu49(p84.Name or "") == vu49(v85) then
            return true
        end
        if string.find(vu49(p84.Name or ""), vu49(v85), 1, true) then
            return true
        end
        local v86 = vu71(p84)
        return v86 and vu49(v86) == vu49(v85) and true or false
    end
    local function vu89(p88)
