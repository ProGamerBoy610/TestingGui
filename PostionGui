    function GradientText(p7, p8, p9)
        _G.result = ""
        for v10 = 1, # p7 do
            _G.t = (v10 - 1) / (# p7 - 1)
            _G.r = p8.R + (p9.R - p8.R) * _G.t
            _G.g = p8.G + (p9.G - p8.G) * _G.t
            _G.b = p8.B + (p9.B - p8.B) * _G.t
            _G.hex = string.format("#%02X%02X%02X", _G.r * 255, _G.g * 255, _G.b * 255)
            _G.result = _G.result .. string.format("<font color=\"%s\">%s</font>", _G.hex, p7:sub(v10, v10))
        end
        return _G.result
    end
