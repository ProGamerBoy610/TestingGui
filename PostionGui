        "AttackDamagePotion1",
        "HealthPotion1",
        "MinerPotion1",
        "MovementSpeedPotion1"
    }
    _G.POTION_BUY_POS_A = Vector3.new(- 164.4535369873047, 28.127897262573242, 123.30853271484375)
    _G.POTION_BUY_POS_B = Vector3.new(- 141.32115173339844, 28.473960876464844, 111.4219741821289)
    _G.AutoBuyPotionsEnabled = false
    _G.PotionToBuy = _G.POTION_OPTIONS[1]
    _G.PotionBuyAmount = 3
    _G.PotionBuyDelay = 0.6
    _G.PotionTweenTime = 0.65
    _G.PotionNoClipDuringTween = true
    _G.PotionVerbose = false
    _G.PurchaseRemote = nil
    _G.PurchaseRemotePathChecked = false
    _G._AutoBuyCoroutine = nil
    _G._AutoBuyCancel = false
    _G._AutoBuyActiveTween = nil
    _G._AutoBuyNoClipParts = {}
    function _G.FindPurchaseRemote()
        if _G.PurchaseRemotePathChecked and _G.PurchaseRemote then
            return _G.PurchaseRemote
        else
            _G.PurchaseRemotePathChecked = true
            local v605, v606 = pcall(function()
                local v601 = _G.ReplicatedStorage:FindFirstChild("Shared") and _G.ReplicatedStorage.Shared:FindFirstChild("Packages")
                if v601 then
                    v601 = _G.ReplicatedStorage.Shared.Packages:FindFirstChild("Knit")
                end
                if v601 then
                    local v602 = v601:FindFirstChild("Services")
                    if v602 then
                        local v603 = v602:FindFirstChild("ProximityService")
                        if v603 then
                            local v604 = v603:FindFirstChild("RF")
                            if v604 then
                                return v604:FindFirstChild("Purchase")
                            else
                                return nil
                            end
                        else
                            return nil
                        end
                    else
                        return nil
                    end
                else
                    return nil
                end
            end)
            if v605 and v606 then
                _G.PurchaseRemote = v606
                return v606
            else
                local v607, v608 = pcall(function()
                    return _G.ReplicatedStorage.Shared.Packages.Knit.Services.ProximityService.RF.Purchase
                end)
                if v607 and v608 then
                    _G.PurchaseRemote = v608
                    return v608
                else
                    _G.PurchaseRemote = nil
                    return nil
                end
            end
        end
    end
    function _G.EnablePotionNoClip()
        if _G.PotionNoClipDuringTween then
            local v609 = _G.Players.LocalPlayer
            if v609 then
                v609 = _G.Players.LocalPlayer.Character
            end
            if v609 then
                _G._AutoBuyNoClipParts = {}
                local v610, v611, v612 = ipairs(v609:GetDescendants())
                while true do
                    local v613
                    v612, v613 = v610(v611, v612)
                    if v612 == nil then
                        break
                    end
                    if v613:IsA("BasePart") then
                        _G._AutoBuyNoClipParts[v613] = v613.CanCollide
                        v613.CanCollide = false
                    end
                end
            end
        else
            return
        end
    end
    function _G.DisablePotionNoClip()
        local v614 = _G.Players.LocalPlayer
        if v614 then
            v614 = _G.Players.LocalPlayer.Character
        end
        if v614 then
            local v615, v616, v617 = pairs(_G._AutoBuyNoClipParts)
            while true do
                local vu618, vu619 = v615(v616, v617)
                if vu618 == nil then
                    break
                end
                v617 = vu618
                if vu618 and vu618.Parent then
                    pcall(function()
                        vu618.CanCollide = vu619
                    end)
                end
            end
            _G._AutoBuyNoClipParts = {}
        else
            _G._AutoBuyNoClipParts = {}
        end
    end
    function _G.CancelAutoBuyTween()
        if _G._AutoBuyActiveTween then
            pcall(function()
                _G._AutoBuyActiveTween:Cancel()
            end)
            _G._AutoBuyActiveTween = nil
        end
    end
    function _G.TweenHRPTo(p620)
        local v621 = _G.Players.LocalPlayer
        if v621 then
            v621 = _G.Players.LocalPlayer.Character
        end
        if not v621 then
            return false, "no-character"
        end
        local vu622 = v621:FindFirstChild("HumanoidRootPart") or v621.PrimaryPart
        if not vu622 then
            return false, "no-hrp"
        end
        local vu623 = CFrame.new(p620, p620 + Vector3.new(0, 0, 1))
        if _G.PotionNoClipDuringTween then
            _G.EnablePotionNoClip()
        end
        local vu624 = math.max(0.02, tonumber(_G.PotionTweenTime) or 0.6)
        local v626, vu627 = pcall(function()
            local v625 = TweenInfo.new(vu624, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
            return _G.TweenService:Create(vu622, v625, {
                CFrame = vu623
            })
        end)
        if not (v626 and vu627) then
            if _G.PotionNoClipDuringTween then
                _G.DisablePotionNoClip()
            end
            return false, "tween-create-failed"
        end
        _G._AutoBuyActiveTween = vu627
        pcall(function()
            vu627:Play()
        end)
        local vu628 = false
        local vu629 = vu627.Completed:Connect(function()
            vu628 = true
        end)
        local v630 = tick()
        local v631 = vu624
        while not vu628 and tick() - v630 < v631 + 0.25 do
            if _G._AutoBuyCancel or not _G.AutoBuyPotionsEnabled then
                pcall(function()
                    vu627:Cancel()
                end)
                break
            end
            task.wait(0.02)
        end
        pcall(function()
            vu629:Disconnect()
        end)
        _G._AutoBuyActiveTween = nil
        if _G.PotionNoClipDuringTween then
            _G.DisablePotionNoClip()
        end
        return vu628, vu628 and "completed" or "cancelled"
    end
    function _G.TryPurchase(pu632, pu633)
        local vu634 = _G.FindPurchaseRemote()
        if vu634 then
            local v635, v636 = pcall(function()
                return vu634:InvokeServer(pu632, pu633)
            end)
            if v635 then
                return true, v636
            else
                return false, v636
            end
        else
            return false, "no-purchase-remote"
        end
    end
    function _G.StartAutoBuyPotions()
        if not _G._AutoBuyCoroutine then
            _G.AutoBuyPotionsEnabled = true
            if type(_G.PotionToBuy) ~= "table" then
                _G.PotionToBuy = {
                    _G.PotionToBuy
                }
            end
            _G._AutoBuyCancel = false
            _G._AutoBuyCoroutine = task.spawn(function()
                while _G.AutoBuyPotionsEnabled and not _G._AutoBuyCancel do
                    local v637, v638 = _G.TweenHRPTo(_G.POTION_BUY_POS_A)
                    if _G.PotionVerbose then
                        print("Tween to A:", v637, v638)
                    end
                    if _G._AutoBuyCancel then
                        break
                    end
                    local v639 = 0
                    while not _G._AutoBuyCancel do
                        v639 = v639 + 1
                        local v640, v641, v642 = ipairs(_G.PotionToBuy)
                        local v643 = false
                        while true do
                            local v644
                            v642, v644 = v640(v641, v642)
                            if v642 == nil then
                                break
                            end
                            local v645, _ = _G.TryPurchase(v644, tonumber(_G.PotionBuyAmount) or 1)
                            if _G.PotionVerbose then
                                print("Buying:", v644, "->", v645)
                            end
                            if v645 then
                                v643 = true
                            end
                        end
                        if v643 then
                            break
                        end
                        if _G.PotionVerbose then
                            print(("Purchase attempt #%d -> ok=%s res=%s"):format(v639, tostring(ok), tostring(res)))
                        end
                        if ok then
                            break
                        end
                        task.wait(math.max(0.05, tonumber(_G.PotionBuyDelay) or 0.6))
                        if v639 >= 60 then
                            if _G.PotionVerbose then
                                warn("AutoBuyPotions: giving up after many attempts, will continue loop.")
                            end
                            break
                        end
                    end
                    if _G._AutoBuyCancel then
                        break
                    end
                    task.wait(0.12)
                    local v646, v647 = _G.TweenHRPTo(_G.POTION_BUY_POS_B)
                    if _G.PotionVerbose then
                        print("Tween to B:", v646, v647)
                    end
                    if _G._AutoBuyCancel then
                        break
                    end
                    local v648 = 0
                    while not _G._AutoBuyCancel do
                        v648 = v648 + 1
                        local v649, v650, v651 = ipairs(_G.PotionToBuy)
                        local v652 = false
                        while true do
                            local v653
                            v651, v653 = v649(v650, v651)
                            if v651 == nil then
                                break
                            end
                            local v654, _ = _G.TryPurchase(v653, tonumber(_G.PotionBuyAmount) or 1)
                            if _G.PotionVerbose then
                                print("Buying:", v653, "->", v654)
                            end
                            if v654 then
                                v652 = true
                            end
                        end
                        if v652 then
                            break
                        end
                        if _G.PotionVerbose then
                            print(("Purchase attempt (B) #%d -> ok=%s res=%s"):format(v648, tostring(ok), tostring(res)))
                        end
                        if ok then
                            break
                        end
                        task.wait(math.max(0.05, tonumber(_G.PotionBuyDelay) or 0.6))
                        if v648 >= 60 then
                            if _G.PotionVerbose then
                                warn("AutoBuyPotions: giving up after many attempts (B), continuing loop.")
                            end
                            break
                        end
                    end
                    task.wait(0.18)
                end
                _G._AutoBuyCoroutine = nil
                _G._AutoBuyCancel = false
                _G.AutoBuyPotionsEnabled = false
                _G.CancelAutoBuyTween()
                _G.DisablePotionNoClip()
                if _G.PotionVerbose then
                    print("AutoBuyPotions loop ended")
                end
            end)
        end
    end
    function _G.StopAutoBuyPotions()
        _G._AutoBuyCancel = true
        _G.AutoBuyPotionsEnabled = false
        _G.CancelAutoBuyTween()
        _G.DisablePotionNoClip()
        _G._AutoBuyCoroutine = nil
    end
