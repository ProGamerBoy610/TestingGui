if not game:IsLoaded() then
    game.Loaded:Wait()
end

task.wait(1)

local function runInChunks(fn)
    local co = coroutine.create(fn)

    task.spawn(function()
        while coroutine.status(co) ~= "dead" do
            local ok, err = coroutine.resume(co)
            if not ok then
                warn("Runtime error:", err)
                return
            end
            task.wait()
        end
    end)
end

runInChunks(function()
    local ok, src = pcall(function()
        return game:HttpGet(
            "https://raw.githubusercontent.com/ProGamerBoy610/TestingGui/main/obfuscated_advanced.lua%20(3).txt",
            true
        )
    end)

    if not ok or type(src) ~= "string" or #src == 0 then
        warn("HttpGet failed or returned empty data")
        return
    end

    local fn, err = loadstring(src)
    if not fn then
        warn("Loadstring error:", err)
        return
    end

    fn()
end)
