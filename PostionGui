-- Load the Tora UI Library
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/liebertsx/Tora-Library/main/src/librarynew", true))()

-- Create a main window
local mainWindow = library:CreateWindow("Your Title")

-- Create a folder (tab) inside the main window
local folder = mainWindow:AddFolder("Main Folder")

-- Add a button inside the folder
folder:AddButton({
    text = "Click me",
    flag = "button",
    callback = function()
        print("hello world")
    end
})

-- Add a label inside the folder
folder:AddLabel({
    text = "This Is Sick!",
    type = "label"
})

-- Initialize and show the GUI
library:Init()
