                            local vu545 = vu530(v543, vu540, vu470)
                            local v546 = (vu545.Position - vu542.Position).Magnitude
                            if vu467 >= v546 then
                                if vu465 then
                                    if vu466 <= (vu465.Position - vu545.Position).Magnitude then
                                        vu465 = vu545
                                    end
