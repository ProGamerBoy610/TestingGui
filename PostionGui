            return pu90:GetAttribute("Drops")
        end)
        if v92 and v93 then
            if type(v93) == "table" then
                local v94, v95, v96 = ipairs(v93)
                while true do
                    local v97
                    v96, v97 = v94(v95, v96)
                    if v96 == nil then
                        break
                    end
                    table.insert(v91, tostring(v97))
                end
                return v91
            end
            if type(v93) == "string" then
                table.insert(v91, tostring(v93))
                return v91
            end
        end
        local v98, v99, v100 = ipairs(pu90:GetDescendants())
        while true do
            local v101
            v100, v101 = v98(v99, v100)
            if v100 == nil then
                if # v91 ~= 0 then
                    return v91
                else
                    return nil
                end
            end
            if v101:IsA("BasePart") or (v101:IsA("Model") or v101:IsA("Folder")) then
                local v102 = v101.Name or ""
                local v103, v104, v105 = ipairs(vu23)
                while true do
                    local v106
                    v105, v106 = v103(v104, v105)
                    if v105 == nil then
                        break
                    end
                    if vu49(v102) == vu49(v106) or string.find(vu49(v102), vu49(v106), 1, true) then
                        table.insert(v91, v102)
                        break
                    end
                end
            end
        end
    end
