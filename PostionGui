-- Create Sidebar Buttons
local function CreateSidebarButton(parent, text, icon, position, callback)
    local Button = Instance.new("TextButton")
    Button.Name = text .. "Button"
    Button.Parent = parent
    Button.BackgroundColor3 = HubConfig.Colors.Secondary
    Button.BorderSizePixel = 0
    Button.Position = UDim2.new(0, 0, 0, position)
    Button.Size = UDim2.new(1, 0, 0, 45)
    Button.AutoButtonColor = false
    
    local ButtonText = Instance.new("TextLabel")
    ButtonText.Name = "ButtonText"
    ButtonText.Parent = Button
    ButtonText.BackgroundTransparency = 1
    ButtonText.Position = UDim2.new(0, 35, 0, 0)
    ButtonText.Size = UDim2.new(1, -35, 1, 0)
    ButtonText.Font = Enum.Font.GothamMedium
    ButtonText.Text = string.upper(text)
    ButtonText.TextColor3 = HubConfig.Colors.TextSecondary
    ButtonText.TextSize = 11
    ButtonText.TextXAlignment = Enum.TextXAlignment.Left
    
    local IconLabel = Instance.new("TextLabel")
    IconLabel.Name = "IconLabel"
    IconLabel.Parent = Button
    IconLabel.BackgroundTransparency = 1
    IconLabel.Position = UDim2.new(0, 12, 0, 0)
    IconLabel.Size = UDim2.new(0, 20, 1, 0)
    IconLabel.Font = Enum.Font.GothamBold
    IconLabel.Text = icon
    IconLabel.TextColor3 = HubConfig.Colors.TextSecondary
    IconLabel.TextSize = 16
    
    -- Hover Effect
    Button.MouseEnter:Connect(function()
        TweenService:Create(Button, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(55, 55, 70)}):Play()
        TweenService:Create(ButtonText, TweenInfo.new(0.2), {TextColor3 = HubConfig.Colors.Text}):Play()
        TweenService:Create(IconLabel, TweenInfo.new(0.2), {TextColor3 = HubConfig.Colors.Accent}):Play()
    end)
    
    Button.MouseLeave:Connect(function()
        TweenService:Create(Button, TweenInfo.new(0.2), {BackgroundColor3 = HubConfig.Colors.Secondary}):Play()
        TweenService:Create(ButtonText, TweenInfo.new(0.2), {TextColor3 = HubConfig.Colors.TextSecondary}):Play()
        TweenService:Create(IconLabel, TweenInfo.new(0.2), {TextColor3 = HubConfig.Colors.TextSecondary}):Play()
    end)
    
    Button.MouseButton1Click:Connect(callback)
    
    return Button
end

-- Create Content Items
local function CreateToggleButton(parent, scriptData, position)
    local Container = Instance.new("Frame")
    Container.Name = "ToggleContainer"
    Container.Parent = parent
    Container.BackgroundColor3 = HubConfig.Colors.Secondary
    Container.BorderSizePixel = 0
    Container.Position = UDim2.new(0, 15, 0, position)
    Container.Size = UDim2.new(1, -30, 0, 45)
    
    local ContainerCorner = Instance.new("UICorner")
    ContainerCorner.CornerRadius = UDim.new(0, 6)
    ContainerCorner.Parent = Container
    
    local Title = Instance.new("TextLabel")
    Title.Name = "Title"
    Title.Parent = Container
    Title.BackgroundTransparency = 1
    Title.Position = UDim2.new(0, 15, 0, 5)
    Title.Size = UDim2.new(1, -80, 0, 20)
    Title.Font = Enum.Font.GothamMedium
    Title.Text = scriptData.name
    Title.TextColor3 = HubConfig.Colors.Text
    Title.TextSize = 13
    Title.TextXAlignment = Enum.TextXAlignment.Left
    
    local Desc = Instance.new("TextLabel")
    Desc.Name = "Desc"
    Desc.Parent = Container
    Desc.BackgroundTransparency = 1
    Desc.Position = UDim2.new(0, 15, 0, 22)
    Desc.Size = UDim2.new(1, -80, 0, 15)
    Desc.Font = Enum.Font.Gotham
    Desc.Text = scriptData.desc
    Desc.TextColor3 = HubConfig.Colors.TextSecondary
    Desc.TextSize = 10
    Desc.TextXAlignment = Enum.TextXAlignment.Left
    
    -- Toggle Switch
    local ToggleFrame = Instance.new("Frame")
    ToggleFrame.Name = "ToggleFrame"
    ToggleFrame.Parent = Container
    ToggleFrame.BackgroundColor3 = scriptData.enabled and HubConfig.Colors.Success or Color3.fromRGB(60, 60, 60)
    ToggleFrame.BorderSizePixel = 0
    ToggleFrame.Position = UDim2.new(1, -55, 0.5, -10)
    ToggleFrame.Size = UDim2.new(0, 40, 0, 20)
    
    local ToggleCorner = Instance.new("UICorner")
    ToggleCorner.CornerRadius = UDim.new(0, 10)
    ToggleCorner.Parent = ToggleFrame
    
    local ToggleKnob = Instance.new("Frame")
    ToggleKnob.Name = "ToggleKnob"
    ToggleKnob.Parent = ToggleFrame
    ToggleKnob.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    ToggleKnob.BorderSizePixel = 0
    ToggleKnob.Position = scriptData.enabled and UDim2.new(0, 22, 0, 2) or UDim2.new(0, 2, 0, 2)
    ToggleKnob.Size = UDim2.new(0, 16, 0, 16)
    
    local KnobCorner = Instance.new("UICorner")
    KnobCorner.CornerRadius = UDim.new(0, 8)
    KnobCorner.Parent = ToggleKnob
    
    -- Toggle Function
    local ToggleButton = Instance.new("TextButton")
    ToggleButton.Name = "ToggleButton"
    ToggleButton.Parent = ToggleFrame
    ToggleButton.BackgroundTransparency = 1
    ToggleButton.Size = UDim2.new(1, 0, 1, 0)
    ToggleButton.Text = ""
    
    ToggleButton.MouseButton1Click:Connect(function()
        scriptData.enabled = not scriptData.enabled
        
        -- Animate toggle
        TweenService:Create(ToggleFrame, TweenInfo.new(0.3), {
            BackgroundColor3 = scriptData.enabled and HubConfig.Colors.Success or Color3.fromRGB(60, 60, 60)
        }):Play()
        
        TweenService:Create(ToggleKnob, TweenInfo.new(0.3), {
            Position = scriptData.enabled and UDim2.new(0, 22, 0, 2) or UDim2.new(0, 2, 0, 2)
        }):Play()
        
        -- Execute script logic here
        print(scriptData.name .. " is now " .. (scriptData.enabled and "enabled" or "disabled"))
        
        -- Add your script execution logic here
        if scriptData.name == "Speed Hack" then
            if scriptData.enabled and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                LocalPlayer.Character.Humanoid.WalkSpeed = 100
            elseif LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                LocalPlayer.Character.Humanoid.WalkSpeed = 16
            end
        elseif scriptData.name == "Jump Boost" then
            if scriptData.enabled and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                LocalPlayer.Character.Humanoid.JumpPower = 200
            elseif LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                LocalPlayer.Character.Humanoid.JumpPower = 50
            end
        end
    end)
    
    return Container
end

local function CreateActionButton(parent, scriptData, position)
    local Button = Instance.new("TextButton")
    Button.Name = "ActionButton"
    Button.Parent = parent
    Button.BackgroundColor3 = HubConfig.Colors.Accent
    Button.BorderSizePixel = 0
    Button.Position = UDim2.new(0, 15, 0, position)
    Button.Size = UDim2.new(1, -30, 0, 40)
    Button.AutoButtonColor = false
    
    local ButtonCorner = Instance.new("UICorner")
    ButtonCorner.CornerRadius = UDim.new(0, 6)
    ButtonCorner.Parent = Button
    
    local Title = Instance.new("TextLabel")
    Title.Name = "Title"
    Title.Parent = Button
    Title.BackgroundTransparency = 1
    Title.Position = UDim2.new(0, 15, 0, 0)
    Title.Size = UDim2.new(1, -15, 0.6, 0)
    Title.Font = Enum.Font.GothamMedium
    Title.Text = scriptData.name
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.TextSize = 13
    Title.TextXAlignment = Enum.TextXAlignment.Left
    Title.TextYAlignment = Enum.TextYAlignment.Bottom
    
    local Desc = Instance.new("TextLabel")
    Desc.Name = "Desc"
    Desc.Parent = Button
    Desc.BackgroundTransparency = 1
    Desc.Position = UDim2.new(0, 15, 0.6, 0)
    Desc.Size = UDim2.new(1, -15, 0.4, 0)
    Desc.Font = Enum.Font.Gotham
    Desc.Text = scriptData.desc
    Desc.TextColor3 = Color3.fromRGB(200, 200, 255)
    Desc.TextSize = 10
    Desc.TextXAlignment = Enum.TextXAlignment.Left
    Desc.TextYAlignment = Enum.TextYAlignment.Top
    
    -- Hover Effects
    Button.MouseEnter:Connect(function()
        TweenService:Create(Button, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(120, 170, 255)}):Play()
    end)
    
    Button.MouseLeave:Connect(function()
        TweenService:Create(Button, TweenInfo.new(0.2), {BackgroundColor3 = HubConfig.Colors.Accent}):Play()
    end)
    
    Button.MouseButton1Click:Connect(function()
        print("Executed: " .. scriptData.name)
        
        -- Add your script execution logic here
        if scriptData.name == "Teleport Tool" then
            print("Teleport tool activated")
        elseif scriptData.name == "Money Generator" then
            print("Money generated")
        end
    end)
    
    return Button
end

-- Populate Content Area
local function PopulateContent(scrollFrame, category, headerText)
    -- Clear existing content
    for _, child in pairs(scrollFrame:GetChildren()) do
        if child:IsA("Frame") or child:IsA("TextButton") then
            child:Destroy()
        end
    end
    
    headerText.Text = string.upper(category) .. " SCRIPTS"
    
    local yPosition = 15
    local scripts = Scripts[category] or {}
    
    for _, scriptData in pairs(scripts) do
        if scriptData.type == "toggle" then
            CreateToggleButton(scrollFrame, scriptData, yPosition)
            yPosition = yPosition + 55
        elseif scriptData.type == "button" then
            CreateActionButton(scrollFrame, scriptData, yPosition)
            yPosition = yPosition + 50
        end
    end
    
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, yPosition + 15)
end

-- Main Execution
local function InitializeHub()
    local screenGui, mainFrame, sidebar, scrollFrame, headerText = CreateMainGUI()
    
    local currentCategory = "EXECUTOR"
    
    -- Create Sidebar Buttons
    local buttonData = {
        {text = "HOME", icon = "🏠", category = "HOME"},
        {text = "EXECUTOR", icon = "⚡", category = "EXECUTOR"},
        {text = "FEATURES", icon = "🎯", category = "FEATURES"},
        {text = "SPAWNER", icon = "📦", category = "SPAWNER"},
        {text = "TELEPORT", icon = "📍", category = "TELEPORT"},
        {text = "SETTINGS", icon = "⚙️", category = "SETTINGS"}
    }
    
    for i, data in pairs(buttonData) do
        CreateSidebarButton(sidebar, data.text, data.icon, (i-1) * 45, function()
            currentCategory = data.category
            PopulateContent(scrollFrame, currentCategory, headerText)
        end)
    end
    
    -- Initialize with first category
    PopulateContent(scrollFrame, currentCategory, headerText)
    
    -- Close Button Functionality
    mainFrame.TitleBar.CloseButton.MouseButton1Click:Connect(function()
        screenGui:Destroy()
    end)
    
    -- Minimize Button Functionality
    local minimized = false
    mainFrame.TitleBar.MinimizeButton.MouseButton1Click:Connect(function()
        minimized = not minimized
        if minimized then
            TweenService:Create(mainFrame, TweenInfo.new(0.3), {Size = UDim2.new(0, 600, 0, 50)}):Play()
        else
            TweenService:Create(mainFrame, TweenInfo.new(0.3), {Size = UDim2.new(0, 600, 0, 400)}):Play()
        end
    end)
    
    -- Drag Functionality
    local dragging = false
    local dragInput, mousePos, framePos
    
    mainFrame.TitleBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            mousePos = input.Position
            framePos = mainFrame.Position
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            local delta = input.Position - mousePos
            mainFrame.Position = UDim2.new(framePos.X.Scale, framePos.X.Offset + delta.X, framePos.Y.Scale, framePos.Y.Offset + delta.Y)
        end
    end)
    
    mainFrame.TitleBar.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
    
    print("🚀 PHANTOM HUB loaded successfully!")
end

-- Launch the hub
InitializeHub()
