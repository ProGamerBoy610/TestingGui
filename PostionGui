    local function vu442()
        if not vu385 then
            vu385 = coroutine.create(function()
                while _G.AutoSellEnabled do
                    pcall(function()
                        vu422()
                    end)
                    task.wait(vu391)
                    local v429, v430 = vu410()
                    if v429 then
                        vu426(2.5)
                        task.wait(vu391)
                        if _G.ItemsToSell and # _G.ItemsToSell > 0 then
                            local v431, v432, v433 = ipairs(_G.ItemsToSell)
                            while true do
                                local v434
                                v433, v434 = v431(v432, v433)
                                if v433 == nil or not _G.AutoSellEnabled then
                                    break
                                end
                                local v435, v436 = vu417(v434, _G.SellAmount or 1)
                                if not v435 then
                                    warn("[AutoSell] failed to sell", v434, v436)
                                end
                                task.wait((_G.SellDelayMs or 70) / 1000 + vu392)
                            end
                        else
                            task.wait(0.5)
                        end
                        local vu437 = vu405()
                        local vu438 = vu401()
                        if vu437 then
                            pcall(function()
                                vu437:FireServer("Closed")
                            end)
                        end
                        if vu438 then
                            pcall(function()
                                vu438:InvokeServer("Close")
                            end)
                        end
                        task.wait(0.25)
                        local v439 = vu373.Character
                        if v439 then
                            v439 = v439:FindFirstChild("HumanoidRootPart")
                        end
                        if v439 then
                            local v440 = TweenInfo.new(vu428, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
                            local v441 = TweenService:Create(v439, v440, {
                                CFrame = CFrame.new(vu427)
                            })
                            v441:Play()
                            v441.Completed:Wait()
                        end
                        task.wait(0.6)
                    else
                        warn("[AutoSell] openDialogueWithRetries failed:", v430)
                        task.wait(0.8)
                    end
                    task.wait(0.2)
                end
                vu385 = nil
            end)
            coroutine.resume(vu385)
        end
    end
