            end)(),
            Value = {},
            Multi = true,
            Callback = function(p700)
                if type(p700) ~= "table" then
                    _G.AutoDrinkSelectedPotions = {
                        tostring(p700)
                    }
                else
                    local v701 = {}
                    if # p700 <= 0 then
                        if p700.Title then
                            table.insert(v701, p700.Title)
                        end
                    else
                        local v702, v703, v704 = ipairs(p700)
                        while true do
                            local v705
                            v704, v705 = v702(v703, v704)
                            if v704 == nil then
                                break
                            end
                            if type(v705) == "table" and v705.Title then
                                table.insert(v701, v705.Title)
                            end
                        end
                    end
                    _G.AutoDrinkSelectedPotions = v701
                end
                print("AutoDrinkSelectedPotions ->", table.concat(_G.AutoDrinkSelectedPotions, ", "))
            end
        })
        v600:Toggle({
            Title = "Auto Drink Potions",
            Desc = "Enable/Disable auto drinking of selected potions.",
            Value = false,
            Callback = function(p706)
                if p706 then
                    _G.StartAutoDrinkLoop()
                else
                    _G.StopAutoDrinkLoop()
                end
            end
        })
        v600:Divider()
        v600:Toggle({
            Title = "Auto Drink Health Potions",
            Desc = "When enabled, automatically drinks HealthPotion1 if health% is below threshold.",
            Value = _G.AutoDrinkHealthEnabled or false,
            Callback = function(p707)
                _G.AutoDrinkHealthEnabled = p707
                if p707 then
                    _G.StartAutoDrinkHealthLoop()
                end
            end
        })
        v600:Slider({
            Title = "Health % Threshold (auto drink)",
            Desc = "Auto  heal when below this slider",
            Value = {
                Min = 1,
                Max = 90,
                Default = _G.AutoDrinkHealthThreshold or 45
            },
            Step = 1,
            Callback = function(p708)
                local v709 = vu688(p708, _G.AutoDrinkHealthThreshold or 45)
                _G.AutoDrinkHealthThreshold = math.clamp(tonumber(v709) or 45, 1, 95)
            end
        })
        v600:Slider({
            Title = "Auto-Drink Delay (s)",
            Desc = "Delay between drink attempts (per potion).",
            Value = {
                Min = 0.05,
                Max = 3,
                Default = _G.AutoDrinkDelay
            },
            Step = 0.05,
            Callback = function(p710)
                local v711 = _G
                local v712 = tonumber
                if type(p710) == "table" then
                    p710 = p710.Default or p710
                end
                v711.AutoDrinkDelay = v712(p710) or _G.AutoDrinkDelay
                print("AutoDrinkDelay ->", _G.AutoDrinkDelay)
            end
        })
    end
    function _G.EnableAutoDrink()
        _G.StartAutoDrinkLoop()
        _G.AutoDrinkPotionsEnabled = true
    end
    function _G.DisableAutoDrink()
        _G.StopAutoDrinkLoop()
    end
