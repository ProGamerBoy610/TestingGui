    _G.PickaxeToBuy = _G.PickaxeToBuy or "Bronze Pickaxe"
    _G.PickaxeTweenTime = _G.PickaxeTweenTime or 0.9
    _G.PickaxeBuyDelay = _G.PickaxeBuyDelay or 0.5
