                                v212 = true
                            end
                        end
                        if v212 then
                            break
                        end
                    end
                    if not v212 then
                        v207 = true
                    end
                end
            end
            if v207 then
                v202 = nil
                vu148 = nil
                vu140(false)
                task.wait(0.05)
