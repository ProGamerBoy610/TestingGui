-- Load WindUI Library
local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

WindUI:SetNotificationLower(true)

-- Add Custom Theme
WindUI:AddTheme({
    Name = "My Theme",
    Accent = Color3.fromHex("#18181b"),
    Dialog = Color3.fromHex("#161616"),
    Outline = Color3.fromHex("#FFFFFF"),
    Text = Color3.fromHex("#FFFFFF"),
    Placeholder = Color3.fromHex("#7a7a7a"),
    Background = Color3.fromHex("#101010"),
    Button = Color3.fromHex("#52525b"),
    Icon = Color3.fromHex("#a1a1aa")
})

-- Create Main Window
local Window = WindUI:CreateWindow({
    Title = "My Super Hub",
    Icon = "door-open",
    Author = "by .ftgs and .ftgs",
    Folder = "MySuperHub",
    Size = UDim2.fromOffset(580, 460),
    MinSize = Vector2.new(560, 350),
    MaxSize = Vector2.new(850, 560),
    Transparent = true,
    Theme = "My Theme",
    Resizable = true,
    SideBarWidth = 200,
    BackgroundImageTransparency = 0.42,
    HideSearchBar = false,
    ScrollBarEnabled = true,
    User = {
        Enabled = true,
        Anonymous = true,
        Callback = function()
            print("clicked")
        end,
    },
})

-- Edit Open Button
Window:EditOpenButton({
    Title = "Open Example UI",
    Icon = "monitor",
    CornerRadius = UDim.new(0,16),
    StrokeThickness = 2,
    Color = ColorSequence.new(
        Color3.fromHex("FF0F7B"), 
        Color3.fromHex("F89B29")
    ),
    OnlyMobile = false,
    Enabled = true,
    Draggable = true,
})

-- Add Version Tag
Window:Tag({
    Title = "v1.6.4",
    Color = Color3.fromHex("#30ff6a"),
    Radius = 8,
})

-- Send Notification
WindUI:Notify({
    Title = "Notification Title",
    Content = "Notification Content example!",
    Duration = 3,
    Icon = "bird",
})

-- ==================== TAB 1: BUTTONS & TOGGLES ====================
local Tab1 = Window:Tab({
    Title = "Buttons & Toggles",
    Icon = "square-check-big",
    Locked = false,
})

Tab1:Select()

local Section1 = Tab1:Section({
    Title = "Button Examples",
    Icon = "circle",
    Opened = true,
})

local Button1 = Tab1:Button({
    Title = "Button 1",
    Desc = "First Test Button",
    Locked = false,
    Callback = function()
        print("Button 1 clicked")
    end
})

local Button2 = Tab1:Button({
    Title = "Button 2",
    Desc = "Second Test Button",
    Locked = false,
    Callback = function()
        print("Button 2 clicked")
    end
})

local Button3 = Tab1:Button({
    Title = "Button 3",
    Desc = "Third Test Button",
    Locked = false,
    Callback = function()
        print("Button 3 clicked")
    end
})

Tab1:Divider()

local Section2 = Tab1:Section({
    Title = "Toggle Examples",
    Icon = "toggle-left",
    Opened = true,
})

local Toggle1 = Tab1:Toggle({
    Title = "Toggle Checkbox 1",
    Desc = "First Checkbox Toggle",
    Icon = "check",
    Type = "Checkbox",
    Default = false,
    Callback = function(state)
        print("Toggle 1: " .. tostring(state))
    end
})

local Toggle2 = Tab1:Toggle({
    Title = "Toggle Checkbox 2",
    Desc = "Second Checkbox Toggle",
    Icon = "check-check",
    Type = "Checkbox",
    Default = false,
    Callback = function(state)
        print("Toggle 2: " .. tostring(state))
    end
})

local Toggle3 = Tab1:Toggle({
    Title = "Toggle Switch 1",
    Desc = "First Switch Toggle",
    Icon = "power",
    Type = "Switch",
    Default = false,
    Callback = function(state)
        print("Toggle 3: " .. tostring(state))
    end
})

local Toggle4 = Tab1:Toggle({
    Title = "Toggle Switch 2",
    Desc = "Second Switch Toggle",
    Icon = "zap",
    Type = "Switch",
    Default = true,
    Callback = function(state)
        print("Toggle 4: " .. tostring(state))
    end
})

Tab1:Space()

-- ==================== TAB 2: DROPDOWNS ====================
local Tab2 = Window:Tab({
    Title = "Dropdowns",
    Icon = "chevron-down",
    Locked = false,
})

local Section3 = Tab2:Section({
    Title = "Dropdown Examples",
    Icon = "list",
    Opened = true,
})

local Dropdown1 = Tab2:Dropdown({
    Title = "Dropdown Single 1",
    Values = { "Option A", "Option B", "Option C", "Option D" },
    Value = "Option A",
    Callback = function(option)
        print("Dropdown 1: " .. option)
    end
})

local Dropdown2 = Tab2:Dropdown({
    Title = "Dropdown Single 2",
    Values = { "Red", "Blue", "Green", "Yellow", "Purple" },
    Value = "Red",
    Callback = function(option)
        print("Dropdown 2: " .. option)
    end
})

Tab2:Divider()

local Section4 = Tab2:Section({
    Title = "Multi-Select Dropdowns",
    Icon = "check-square",
    Opened = true,
})

local Dropdown3 = Tab2:Dropdown({
    Title = "Dropdown Multi 1",
    Values = { "Category A", "Category B", "Category C", "Category D" },
    Value = { "Category A" },
    Multi = true,
    AllowNone = true,
    Callback = function(options)
        print("Dropdown 3: " .. game:GetService("HttpService"):JSONEncode(options))
    end
})

local Dropdown4 = Tab2:Dropdown({
    Title = "Dropdown Multi 2",
    Values = { "Item 1", "Item 2", "Item 3", "Item 4", "Item 5" },
    Value = { "Item 1", "Item 2" },
    Multi = true,
    AllowNone = false,
    Callback = function(options)
        print("Dropdown 4: " .. game:GetService("HttpService"):JSONEncode(options))
    end
})

Tab2:Space()

-- ==================== TAB 3: SLIDERS ====================
