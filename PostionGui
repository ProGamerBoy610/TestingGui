-- wait for roblox to fully load
if not game:IsLoaded() then
    game.Loaded:Wait()
end

-- let executor stabilize
task.wait(1)

-- anti-timeout chunk runner
local function runInChunks(fn)
    local co = coroutine.create(fn)

    task.spawn(function()
        while coroutine.status(co) ~= "dead" do
            local ok, err = coroutine.resume(co)
            if not ok then
                warn("Loader error:", err)
                return
            end
            task.wait() -- yield every frame (VERY IMPORTANT)
        end
    end)
end

-- run your big script safely
runInChunks(function()
    local src = game:HttpGet(
        "https://raw.githubusercontent.com/ProGamerBoy610/TestingGui/refs/heads/main/obfuscated_advanced.lua%20(3).txt",
        true
    )

    local fn = loadstring(src)
    fn()
end)
