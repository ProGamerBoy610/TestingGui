    local function vu309()
        if not (workspace:FindFirstChild("Proximity") and workspace.Proximity:FindFirstChild("Marbles")) then
            return nil
        end
        local v308 = workspace.Proximity.Marbles
        return v308:FindFirstChild("HumanoidRootPart") or v308.PrimaryPart
    end
    local function vu312()
        local v310 = vu309()
        if not v310 then
            return false
        end
        if not vu288.Character then
            return false
        end
        local v311 = vu288.Character:FindFirstChild("HumanoidRootPart")
        if not v311 then
            return false
        end
        if (v311.Position - v310.Position).Magnitude <= 6 then
            return true
        end
        vu289 = v311.CFrame
        if not MovementLock then
            MovementLock = true
            tweenToCFrame(v310.CFrame * CFrame.new(0, 0, - 4))
            task.wait(0.4)
            return (v311.Position - v310.Position).Magnitude <= 8
        end
    end
    MovementLock = false
    function tweenToCFrame(p313)
        if not vu288.Character then
            return false
        end
        local v314 = vu288.Character:FindFirstChild("HumanoidRootPart")
        if not v314 then
            return false
        end
        local v315 = (v314.Position - p313.Position).Magnitude
        if v315 > 600 then
            return false
        end
        local v316 = math.clamp(v315 / 10, 1.8, 7)
        local v317 = math.max(1, math.ceil(v315 / 6))
        local v318 = v316 / v317
        for v319 = 1, v317 do
            if not v314.Parent then
                return false
            end
            local v320 = v319 / v317
            local v321 = v314.CFrame:Lerp(p313, v320)
            local v322 = TweenService:Create(v314, TweenInfo.new(v318, Enum.EasingStyle.Linear), {
                CFrame = v321
            })
            v322:Play()
            v322.Completed:Wait()
            task.wait(0.03)
        end
        return true
    end
    local function vu343()
        local v323 = vu307()
        if not (v323 and (v323.Data and (v323.Data.Inventory and v323.Data.Inventory.Equipments))) then
            return {}
        end
        local v324, v325, v326 = pairs(v323.Data.Inventory.Equipments)
        local v327 = {}
        local v328 = 0
        while true do
            local vu329
            v326, vu329 = v324(v325, v326)
            if v326 == nil then
                break
            end
            if type(vu329) == "table" then
                local v330 = vu329.GUID or vu329.Name
                local vu331 = vu329.Type or vu329.Name
                local v332, v333 = pcall(function()
                    return vu293:GetItemType(vu331)
                end)
                if v332 and v333 == "Weapon" then
                    local v334
                    if _G.WeaponTypesToSell and # _G.WeaponTypesToSell > 0 then
                        local v335, v336, v337 = ipairs(_G.WeaponTypesToSell)
                        local v338 = vu331
                        v334 = false
                        while true do
                            local v339
                            v337, v339 = v335(v336, v337)
                            if v337 == nil then
                                break
                            end
                            if tostring(v338):lower():match(tostring(v339):lower()) or tostring(vu329.Name):lower():match(tostring(v339):lower()) then
                                v334 = true
                                break
                            end
                        end
                    else
                        v334 = true
                    end
                    if v334 then
                        local v340 = true
                        if _G.SellBelowPrice and _G.SellBelowPrice > 0 then
                            local v341, v342 = pcall(function()
                                return vu293:CalculatePrice(vu329)
                            end)
                            if (v341 and v342 and v342 or 0) >= _G.SellBelowPrice then
                                v340 = false
                            end
                        end
                        if v340 and (v330 and not v327[v330]) then
                            v327[v330] = true
                            v328 = v328 + 1
                            if _G.ChunkSellLimit and _G.ChunkSellLimit <= v328 then
                                break
                            end
                        end
                    end
                end
            end
        end
        return v327
    end
    local function vu350()
        local vu344 = vu300
        local v345 = vu309()
        if not (vu344 and v345) then
            return false, "no-proximity-or-marbles"
        end
        local vu346 = v345.Parent
        local v347, v348 = pcall(function()
            return vu344:ForceDialogue(vu346, "SellConfirm")
        end)
        local v349
        if v347 then
            v349 = v348 ~= false
        else
            v349 = v347
        end
        return v349, v347 and v348 and v348 or "force-failed"
    end
    local function vu355(pu351)
        if pu351 and next(pu351) then
            if _G.UseSellAnywhereIfAvailable and (vu301 and typeof(vu301.SellAnywhere) == "function") then
                local v352, _ = pcall(function()
                    return vu301:SellAnywhere(pu351, true)
                end)
                if v352 then
                    return true, "sold-via-inventoryservice"
                end
            end
            if vu302 and typeof(vu302.InvokeServer) == "function" then
                local v353, v354 = pcall(function()
                    return vu302:InvokeServer("SellConfirm", {
                        Basket = pu351
                    })
                end)
                if v353 then
                    return true, v354
                else
                    return false, v354
                end
            else
                return false, "no-sell-remote"
            end
        else
            return false, "empty-basket"
        end
    end
    local function vu356()
        if vu302 and typeof(vu302.InvokeServer) == "function" then
            pcall(function()
                vu302:InvokeServer("Close")
            end)
        end
    end
    local vu357 = nil
    local function vu366()
        if not vu357 then
            vu357 = coroutine.create(function()
                while _G.AutoSellWeaponsEnabled do
                    local vu358 = false
                    local function v359()
                        vu358 = true
                    end
                    vu307()
                    local v360 = vu343()
                    if not (v360 and next(v360)) then
                        vu2:Notify({
                            Title = "Auto Sell",
                            Content = "No weapons left to sell. Auto-sell stopped.",
                            Duration = 3,
                            Icon = "bird"
                        })
                        _G.AutoSellWeaponsEnabled = false
                        v359()
                    end
                    if not vu289 and vu288.Character and vu288.Character:FindFirstChild("HumanoidRootPart") then
                        vu289 = vu288.Character.HumanoidRootPart.CFrame
                    end
                    if vu312() then
                        local v361, v362 = vu350()
                        if v361 then
                            task.wait(0.18)
                            if v360 and next(v360) then
                                local v363, v364 = vu355(v360)
                                if v363 then
                                    print("[AutoSellWeapons] Sold basket")
                                    if vu289 then
                                        task.wait(0.15)
                                        tweenToCFrame(vu289, 1.6)
                                        vu289 = nil
                                    end
                                else
                                    warn("[AutoSellWeapons] Sell failed:", tostring(v364))
                                end
                                task.wait((_G.SellDelayMs or 80) / 1000 + 0.12)
                            else
                                task.wait(0.6)
                            end
                            pcall(vu356)
                        else
                            warn("[AutoSellWeapons] Could not open sell dialogue:", tostring(v362))
                            local vu365 = vu309()
                            if vu365 and vu288.Character and vu288.Character:FindFirstChild("HumanoidRootPart") then
                                if vu288.Character and vu288.Character:FindFirstChild("HumanoidRootPart") then
                                    vu289 = vu288.Character.HumanoidRootPart.CFrame
                                end
                                pcall(function()
                                    tweenToCFrame(vu365.CFrame * CFrame.new(0, 0, - 2), 1.6)
                                end)
                                task.wait(0.35)
                            end
                            task.wait(0.9)
                        end
                        task.wait(0.2)
                    else
                        task.wait(1)
                    end
                    if vu358 then
                        break
                    end
                end
                vu357 = nil
            end)
            coroutine.resume(vu357)
        end
    end
    function _G.StartAutoSellWeapons()
